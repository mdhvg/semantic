{
	"documentsDir": {
		"joinHomeDir": true,
		"path": ["Semantic"]
	},
	"documentsPlaintextDir": {
		"joinHomeDir": true,
		"path": ["Semantic", "plaintext"]
	},
	"dbFile": "./documents.sqlite",
	"indexFile": "./index.usearch",

	"serverSource": {
		"url": "https://github.com/mdhvg/semantic-server/archive/refs/heads/main.zip",
		"path": "server",
		"zip": "server.zip",
		"requirements": "server/requirements.txt"
	},

	"pythonServerFile": "server/server.py",

	"setupScript": "server/setup.sh",

	"serverAddress": {
		"host": "127.0.0.1",
		"port": 5096
	},

	"socketServer": {
		"host": "127.0.0.1",
		"port": 5097
	},

	"win32": {
		"python": {
			"url": "https://www.python.org/ftp/python/3.12.8/python-3.12.8-embed-amd64.zip",
			"path": "embedded_python",
			"zip": "embedded_python.zip",
			"binary": "embedded_python/python.exe",
			"flag": "embedded_python/.installed"
		},
		"getpip": {
			"url": "https://bootstrap.pypa.io/get-pip.py",
			"path": "embedded_python/getpip.py"
		}
	},
	"linux": {
		"python": {
			"path": ".env",
			"binary": ".env/bin/python3",
			"flag": ".env/.installed"
		}
	}
}
