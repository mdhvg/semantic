{
	"files": [],
	"include": [
		"src/renderer/src/env.d.ts",
		"src/renderer/src/**/*",
		"src/renderer/src/**/*.svelte",
		"src/preload/*.d.ts",
		"src/shared/**/*",
	],
	"references": [
		{
			"path": "./tsconfig.node.json"
		},
		{
			"path": "./tsconfig.web.json"
		}
	],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$shared/*": [
				"src/shared/*"
			],
			"@/*": [
				"src/renderer/src/*"
			]
		}
	}
}